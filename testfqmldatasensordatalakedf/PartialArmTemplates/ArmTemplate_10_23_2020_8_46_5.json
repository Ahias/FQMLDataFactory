{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "testfqmldatasensordatalakedf"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/fqmdatasandatasensordatalakels')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "https://testfqmldatasensordl.dfs.core.windows.net/",
					"accountKey": {
						"type": "AzureKeyVaultSecret",
						"store": {
							"referenceName": "fqmlsecretskv",
							"type": "LinkedServiceReference"
						},
						"secretName": "sensordatalakekvs"
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/fqmlsecretskv')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/fqmlmlrequestfn')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureFunction",
				"typeProperties": {
					"functionAppUrl": "https://testfqmlmlrequestfn.azurewebsites.net",
					"functionKey": {
						"type": "AzureKeyVaultSecret",
						"store": {
							"referenceName": "fqmlsecretskv",
							"type": "LinkedServiceReference"
						},
						"secretName": "mlkeykvs"
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/fqmlsecretskv')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/fqmlsecretskv')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureKeyVault",
				"typeProperties": {
					"baseUrl": "https://testfqmldatafactorykv.vault.azure.net/"
				}
			},
			"dependsOn": []
		}
	]
}