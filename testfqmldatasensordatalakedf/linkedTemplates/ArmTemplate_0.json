{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "testfqmldatasensordatalakedf"
		},
		"fqmldatasandatasensordatalakedbls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'fqmldatasandatasensordatalakedbls'"
		},
		"pandwhdblisten_edwstageareasqlls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'pandwhdblisten_edwstageareasqlls'"
		},
		"pandwhdblisten_metallurgystreamspredictionsls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'pandwhdblisten_metallurgystreamspredictionsls'"
		},
		"fqmlsecretskv_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://testfqmldatafactorykv.vault.azure.net/"
		},
		"fqmdatasandatasensordatalakels_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://testfqmldatasensordl.dfs.core.windows.net/"
		},
		"fqmlmlrequestfn_properties_typeProperties_functionAppUrl": {
			"type": "string",
			"defaultValue": "https://testfqmlmlrequestfn.azurewebsites.net"
		},
		"pandwhdblisten_edwstageareasqlls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "fqml\\pan_primusdata01"
		},
		"pandwhdblisten_metallurgystreamspredictionsls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "fqml\\pan_primusdata01"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/fqmlsecretskv')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureKeyVault",
				"typeProperties": {
					"baseUrl": "[parameters('fqmlsecretskv_properties_typeProperties_baseUrl')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/fqmldatasensorsqlir')]",
			"type": "Microsoft.DataFactory/factories/integrationRuntimes",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "SelfHosted",
				"typeProperties": {}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/fqmdatasandatasensordatalakels')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('fqmdatasandatasensordatalakels_properties_typeProperties_url')]",
					"accountKey": {
						"type": "AzureKeyVaultSecret",
						"store": {
							"referenceName": "fqmlsecretskv",
							"type": "LinkedServiceReference"
						},
						"secretName": "sensordatalakekvs"
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/fqmlsecretskv')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/fqmldatasandatasensordatalakedbls')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"connectionString": "[parameters('fqmldatasandatasensordatalakedbls_connectionString')]",
					"password": {
						"type": "AzureKeyVaultSecret",
						"store": {
							"referenceName": "fqmlsecretskv",
							"type": "LinkedServiceReference"
						},
						"secretName": "azuresqlsrvpwdkvs"
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/fqmlsecretskv')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/fqmlmlrequestfn')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureFunction",
				"typeProperties": {
					"functionAppUrl": "[parameters('fqmlmlrequestfn_properties_typeProperties_functionAppUrl')]",
					"functionKey": {
						"type": "AzureKeyVaultSecret",
						"store": {
							"referenceName": "fqmlsecretskv",
							"type": "LinkedServiceReference"
						},
						"secretName": "mlkeykvs"
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/fqmlsecretskv')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/pandwhdblisten_edwstageareasqlls')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "linked service (SQL Server) for sever PANDWHDB-LISTEN to database EDWSTAGEArea",
				"annotations": [],
				"type": "SqlServer",
				"typeProperties": {
					"connectionString": "[parameters('pandwhdblisten_edwstageareasqlls_connectionString')]",
					"userName": "[parameters('pandwhdblisten_edwstageareasqlls_properties_typeProperties_userName')]",
					"password": {
						"type": "AzureKeyVaultSecret",
						"store": {
							"referenceName": "fqmlsecretskv",
							"type": "LinkedServiceReference"
						},
						"secretName": "onpremisessqlsrvpwdkvs"
					}
				},
				"connectVia": {
					"referenceName": "fqmldatasensorsqlir",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/fqmldatasensorsqlir')]",
				"[concat(variables('factoryId'), '/linkedServices/fqmlsecretskv')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/pandwhdblisten_metallurgystreamspredictionsls')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "SqlServer",
				"typeProperties": {
					"connectionString": "[parameters('pandwhdblisten_metallurgystreamspredictionsls_connectionString')]",
					"userName": "[parameters('pandwhdblisten_metallurgystreamspredictionsls_properties_typeProperties_userName')]",
					"password": {
						"type": "AzureKeyVaultSecret",
						"store": {
							"referenceName": "fqmlsecretskv",
							"type": "LinkedServiceReference"
						},
						"secretName": "onpremisessqlsrvpwdkvs"
					}
				},
				"connectVia": {
					"referenceName": "fqmldatasensorsqlir",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/fqmldatasensorsqlir')]",
				"[concat(variables('factoryId'), '/linkedServices/fqmlsecretskv')]"
			]
		}
	]
}